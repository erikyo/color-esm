{
  "compilerOptions": {
    "esModuleInterop": true,
    "lib": [
      "es2022"
    ],
    "module": "CommonJS",
    "skipLibCheck": true,
    "target": "ES2022",
    "noImplicitAny": true,
    "preserveConstEnums": true,
    "outDir": "./lib",
    "sourceMap": true,
    "baseUrl": "./",
    "strict": true
  },
  "include": ["./**/*"],
  "exclude": ["**/*.spec.ts"]
}
